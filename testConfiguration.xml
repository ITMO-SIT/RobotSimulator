<?xml version="1.0" encoding="UTF-8"?>
<list>

    <SingleSimulation>
        <Class type = "simulation" value = "simulator.simulation.ready.DefaultSimulation"/>
        <Class type = "robot" value = "simulator.robot.DefaultRobot"/>
        <Class type = "target" value = "simulator.target.CountTarget"/>
    </SingleSimulation>

    <SingleSimulation>
        <Class type = "simulation" value = "simulator.simulation.ready.AnyLogicSimulation"/>
        <Class type = "robot" value = "simulator.robot.AnyLogicRobot"/>
        <Class type = "target" value = "simulator.target.CountTarget"/>

        <ConstParam param = "name" value = "Тестовая симуляция"/>

        <RandomParam paaram = "countPhilistine" min_val = "100" max_val = "1000"/>
    </SingleSimulation>

    <RepeatSimulation>
        <Class type = "simulation" value = "simulator.simulation.ready.AnyLogicSimulation"/>
        <Class type = "robot" value = "simulator.robot.AnyLogicRobot"/>
        <Class type = "target" value = "simulator.target.CountTarget"/>

        <ConstParam param = "name" value = "Тестовая серия симуляций"/>
        <ConstParam param = "resultFilePath" value = "result.txt" />
    </RepeatSimulation>
</list>